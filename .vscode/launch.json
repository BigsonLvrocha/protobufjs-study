{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Server",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/src/server.ts",
      "preLaunchTask": "build",
      "outFiles": ["${workspaceFolder}/dist/**/*.js"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Protobuf Client",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/src/clientProtobufjs.ts",
      "outFiles": ["${workspaceFolder}/dist/**/*.js"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Grpc pure client",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/src/clientGrpcPure.ts",
      "outFiles": ["${workspaceFolder}/dist/**/*.js"]
    }
  ]
}
